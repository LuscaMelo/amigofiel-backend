datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Animal {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  nome      String
  tipo      String
  sexo      String
  porte     String
  idade     Int
  descricao String
  castrado  Boolean
}
